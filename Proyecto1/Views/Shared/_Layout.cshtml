    <title>@ViewBag.Title - Mi App Lavado Vehículos</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
    
 
    <div class="layout-wrapper">

        <header class="navbar">
            <div class="navbar-container">
                <div class="navbar-logo">
                    <a href="@Url.Action("Index", "Home")">Se lo Chaneo a domicilio
                    </a>
                </div>
                <nav class="navbar-links">
                    <a href="@Url.Action("Index", "Clientes")">Clientes</a>
                    <a href="@Url.Action("MostrarEmpleados", "Empleados")">Empleados</a>
                    <a href="@Url.Action("Index", "Vehiculos")">Vehículos</a>

                    <a href="@Url.Action("Index", "Lavados")">Lavados</a>


                </nav>
            </div>
        </header>

        <main role="main" class="container">
            @RenderBody()
        </main>

       
     </div>
        
      

   </body>
</html>